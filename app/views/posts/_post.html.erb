<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Subtitle:</strong>
    <%= post.subtitle %>
  </p>

  <p>
    <strong>Spotify url:</strong>
    <%= post.spotify_url %>
  </p>

  <p>
    <strong>Anchor url:</strong>
    <%= post.anchor_url %>
  </p>

  <p>
    <strong>Excerpt:</strong>
    <%= post.excerpt %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= post.published_at %>
  </p>

</div>
